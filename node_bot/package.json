{
  "name": "poly-btc-bot",
  "version": "1.0.0",
  "description": "Polymarket BTC 15m up/down prediction trading bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "bash scripts/check-and-prepare.sh",
    "prestart": "npm run prepare && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "collect-data": "tsx src/collectData.ts",
    "status": "node scripts/status.js",
    "logs": "tail -f ../logs/bot.log",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@polymarket/clob-client": "^3.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.9.0",
    "onnxruntime-node": "^1.16.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

